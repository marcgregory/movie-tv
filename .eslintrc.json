{
    "extends":[
        "eslint:recommended",
        "plugin:import/error",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:react-hooks/recommended",
        "prettier"
    ],
    "rules":{
        "react/prop-types":0,
        "react/react-in-jsx-scope":0
    },
    "plugins": ["react","jsx-a11y","import","react-hooks"],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env":{
        "es6": true,
        "browser":true,
        "node": true
    },
    "settings": {
        "react":{
            "versions":"detect"
        },
    
    
        "import/resolver":{
            "node":{
                "extensions":[".js,.jsx,.ts,.tsx"]
            }
        }
    
    }
}